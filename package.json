{
  "name": "ultra",
  "version": "0.0.1",
  "description": "Handle a route or two without breaking a sweat!",
  "main": "./lib/ultra.js",
  "module": "./lib/ultra.mjs",
  "scripts": {
    "test": "cross-env BABEL_ENV=test jest --useStderr",
    "cover": "cross-env BABEL_ENV=test jest --coverage",
    "format": "prettier --no-semi --single-quote --write \"{src,test}/**/*.js\"",
    "lint": "npm run format && eslint -c ./config/eslint/.eslintrc.json --ignore-path .gitignore src/**",
    "build": "rollup -c && rollup -c --environment DIST",
    "prepublish": "npm run build"
  },
  "repository": "react-circuit/ultra",
  "author": "Ankit Patel <gt3.someday@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^17.0.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.2.2",
    "jest": "^18.1.0",
    "prettier": "^0.22.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babili": "^2.0.0"
  },
  "peerDependencies": {
    "babel-plugin-dev-expression": "^0.2.1"
  },
  "jest": {
    "testRegex": "(/test/.*\\.test\\.js$)",
    "testEnvironment": "node"
  },
  "dependencies": {
    "warning": "^3.0.0"
  },
  "files": [
    "dist",
    "lib"
  ]
}
